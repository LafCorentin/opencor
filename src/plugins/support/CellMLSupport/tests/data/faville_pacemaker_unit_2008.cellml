        <variable name="CS1" private_interface="in" public_interface="out" units="micromolar"/>
        <variable name="CS2" private_interface="in" public_interface="out" units="micromolar"/>
        <variable name="n_PU" private_interface="out" public_interface="in" units="dimensionless"/>
        <variable name="JS2Cy" private_interface="out" public_interface="in" units="flux"/>
        <variable name="JS1S2" private_interface="out" public_interface="in" units="flux"/>
        <variable name="Vm" private_interface="out" public_interface="in" units="millivolt"/>
        <variable name="time" private_interface="out" public_interface="in" units="second"/>
    </component>
    <connection>
        <map_components component_1="pacemaker_unit_model" component_2="PU_membrane"/>
        <map_variables variable_1="I_ion_PU" variable_2="I_ion_PU"/>
    </connection>
    <connection>
        <map_components component_1="pacemaker_unit_model" component_2="CS2"/>
