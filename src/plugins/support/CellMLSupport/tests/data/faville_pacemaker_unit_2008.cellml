<?xml version='1.0' encoding='UTF-8'?>
<model cmeta:id="faville_pullan_sanders_koh_lloyd_smith_2009_pacemaker" name="faville_pullan_sanders_koh_lloyd_smith_2009_pacemaker" xmlns="http://www.cellml.org/cellml/1.1#" xmlns:cellml="http://www.cellml.org/cellml/1.1#" xmlns:cmeta="http://www.cellml.org/metadata/1.0#">
    <units name="millivolt">
